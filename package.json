{
  "name": "react-tikzcd",
  "version": "0.1.0",
  "description": "React for tikzcd diagrams",
  "main": "./dist/react-tikzcd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yishn/react-tikzcd.git"
  },
  "keywords": [
    "tikzcd",
    "react",
    "diagram",
    "math",
    "latex",
    "tex"
  ],
  "author": "Yichuan Shen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yishn/react-tikzcd/issues"
  },
  "homepage": "https://github.com/yishn/react-tikzcd",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-1"
    ]
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-1"
      ],
      "plugins": [
        [
          "transform-react-jsx",
          {
            "pragma": "h"
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "rollup": "^0.47.0",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-watch": "^4.3.1"
  },
  "scripts": {
    "test": "ava",
    "build": "rollup -c",
    "watch": "rollup -c -w"
  }
}
